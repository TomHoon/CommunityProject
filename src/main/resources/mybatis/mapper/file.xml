<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
	PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.newlecture.web.dao.FileDao">
	<select id="getOneFile" resultType="com.newlecture.web.entity.FileEntity" parameterType="com.newlecture.web.entity.FileEntity">
		select * from file_tb
		where file_idx = #{file_idx}
	</select>
	
</mapper>